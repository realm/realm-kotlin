{
    "problemMatcher": [
        {
            "owner": "detekt",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "(.+\\.kt):(\\d+):(\\d+):\\s(.*)\\s\\[(.+)\\]",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4,
                    "code": 5
                }
            ]
        }
    ]
}

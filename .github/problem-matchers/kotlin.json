{
    "problemMatcher": [
        {
            "owner": "kotlin",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "e:\\s(.+):(\\d+):(\\d+)\\s(.+)",
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "severity": 1,
                    "message": 5
                }
            ]
        },
        {
            "owner": "kotlin",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "w:\\s(.+):(\\d+):(\\d+)\\s(.+)",
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "severity": 1,
                    "message": 5
                }
            ]
        }
    ]
}
